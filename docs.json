{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "Browseagent",
  "colors": {
    "primary": "#4473ff",
    "light": "#f5f3ed",
    "dark": "#4473ff"
  },
  "description": "Give your AI agent browser superpowers",
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "favicon": "/favicon.png",
  "icons": {
    "library": "lucide"
  },
  "eyebrows": "section",
  "font": {
    "family": "Inter",
    "weight": "400"
  },
  "navigation": {
    "interaction": {
      "drilldown": true
    },
    "dropdowns": [
      {
        "dropdown": "Documentation",
        "icon": "book",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "quickstart"]
          },
          {
            "group": "Guides",
            "pages": ["guides/tools-reference","guides/workflows","guides/configuration","guides/troubleshooting"]
          }
        ]
      },
      {
        "dropdown": "API reference",
        "icon": "puzzle",
        "groups": [
          {
            "group": "Getting",
            "pages": []
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://docs.bread.africa"
  },
  "api": {
    "mdx": {
      "auth": {
        "method": "bearer"
      }
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hello@browseagent.pro"
      },
      {
        "label": "Github",
        "href": "https://github.com/browseagent/mcp"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Install",
      "href": "https://chrome.google.com/webstore/detail/browseagent"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/browseagent",
      "github": "https://github.com/browseagent"
    }
  },
  "thumbnails": {
    "background": "/images/thumbnail/background.svg"
  }
}